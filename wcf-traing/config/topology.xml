<?xml version="1.0" encoding="UTF-8"?>
<types>

	<type name = "Car" extends="TopologyObject">
		  <property name="name" type="String" is-identity="true"/>
		  <property name="brand" type="String" is-identity="true"/>
		  <property name="price" type="Double" />
		  <property name="salesVolume" type="Metric" unit-name="count"/>
	</type>
	
	<type name = "DemoUser" extends="TopologyObject">
		  <property name="IDNumber" type="String" is-identity="true"/>
		  <property name="userName" type="String" />
		  <property name="sex" type="Integer"/>
		  <property name="age" type="Integer"/>
		  <property name="phoneNumber" type="String"/>
		  <property name="email" type="String"/>
		  <property name="department" type="String"/>
	</type>
	
</types>